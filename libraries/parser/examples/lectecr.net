#place A mk(2*<..>)
#place B
#place L 
#place EX mk(2*<..>)
#place C mk(1*<..>)
#place D
#place E

#trans t1
in {A:<..>;}
out {B:<..>;}
#endtr

#trans t2
in {B:<..>;EX:<..>;}
out {L:<..>;}
#endtr

#trans t3
in {L:<..>;}
out {A:<..>;EX:<..>;}
#endtr

#trans t4
in {C:<..>;}
out {D:<..>;}
#endtr

#trans t5
in {D:<..>;EX:2*<..>;}
out {E:<..>;}
#endtr

#trans t6
in {E:<..>;}
out {C:<..>;EX:2*<..>;}
#endtr
